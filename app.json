{
  "expo": {
    "name": "통신닥터",
    "slug": "tongdoc_app",
    "version": "1.7.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#ffffff",
      "androidMode": "default",
      "iosDisplayInForeground": true
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/push.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2D63E2"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/a3143539-fa0c-4553-ac08-ecd426063869"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "co.kr.tongdoc",
      "supportsTablet": true,
      "buildNumber": "1.4.1",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsArbitraryLoadsInWebContent": true
        },
        "NSContactsUsageDescription": "연락처 권한이 필요합니다.",
        "NSMessage": "SMS 수신 동의가 필요합니다.",
        "NSLocationWhenInUseUsageDescription": "위치 동의가 필요합니다."
      }
    },
    "android": {
      "package": "kr.co.tongdoc",
      "versionCode": 8,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["DEFAULT", "BROWSABLE"],
          "data": {
            "scheme": "exampleformanagedexpo"
          }
        }
      ],
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "SMS",
        "NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a3143539-fa0c-4553-ac08-ecd426063869"
      }
    }
  }
}
